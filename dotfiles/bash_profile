alias dr="demeter r"
alias dri="demeter r issuances"
alias drm="demeter r main"

alias debian="docker run --rm -it debian bash"

alias em="ember"
alias es="ember serve"
alias ets="ember test --server"

export PATH="/Users/samrichards/projects/demeter:$PATH"
